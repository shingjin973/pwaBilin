{
  "name": "bilinstudio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration prod-en --base-href /en/",
    "start:zh": "ng serve --configuration prod-zh --base-href /ch/",
    "build:en": "ng build --configuration prod-en --base-href /en/",
    "build:zh": "ng build --configuration prod-zh --base-href /ch/",
    "build": "npm run build:en && npm run build:zh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build && firebase deploy --only hosting",
    "lint:functions": "eslint ./functions",
    "extract-i18n": "ng xi18n bilinstudio --i18n-format xlf --output-path ./i18n --i18n-locale en && ng run bilinstudio:xliffmerge"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.0",
