    "@angular/fire": "^5.4.2",
    "@angular/forms": "~8.2.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@angular/service-worker": "~8.2.0",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@sweetalert2/ngx-sweetalert2": "^5.0.1",
    "angular-star-rating": "^4.0.0-beta.3",
    "axios": "^0.18.0",
    "bootstrap": "^4.4.1",
    "browser-image-resizer": "^2.1.0",
    "core-js": "^2.6.11",
    "csvtojson": "^2.0.8",
    "jquery": "^3.5.0",
    "lodash": "^4.17.11",
    "merge-images": "^2.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "ng-recaptcha": "^5.0.0",
    "ngx-bar-rating": "^1.1.0",
    "ngx-cookie-service": "^12.0.3",
    "ngx-file-drop": "^6.0.0",
